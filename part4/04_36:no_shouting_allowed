# Write your solution here
def no_shouting(words):
    new = []
    for char in words:
        if char.isupper() != True:
            new.append(char)
    return new        

if __name__ == "__main__":
    my_list = ["ABC", "def", "UPPER", "ANOTHERUPPER",
               "lower", "another lower", "Capitalized"]
    pruned_list = no_shouting(my_list)
    print(pruned_list)
